<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/SeeMoreActivity"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    android:orientation="vertical"
    android:padding="20dp"
    tools:context=".SeeMoreActivity">

    <LinearLayout
        android:id="@+id/seeMore_LinearLayout_TOP"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:paddingTop="40dp"
        app:layout_constraintTop_toTopOf="parent">

        <ImageButton
            android:id="@+id/seeMore_ImageButton_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#00FF0000"
            android:contentDescription="@string/SeeMore_title"
            app:srcCompat="@drawable/backbutton" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:fontFamily="@font/nanumsquareb"
            android:text="@string/SeeMore_title"
            android:textColor="#000000"
            android:textSize="28dp"
            tools:ignore="SpUsage" />

    </LinearLayout>

    <ScrollView
        android:id="@+id/seeMore_LinearLayout_CENTER"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="40dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/seeMore_LinearLayout_TOP">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <Button
                    android:id="@+id/seeMore_Button_sub0"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    android:background="@drawable/basic_button_gray"
                    android:fontFamily="@font/nanumsquareb"
                    android:paddingStart="20dp"
                    android:paddingEnd="20dp"
                    android:stateListAnimator="@null"
                    android:textAlignment="textStart"
                    android:textColor="#FFFFFF"
                    android:textSize="16dp"
                    android:visibility="invisible"
                    tools:ignore="SpUsage" />

                <Button
                    android:id="@+id/seeMore_Button_sub2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    android:background="@drawable/basic_button_gray"
                    android:fontFamily="@font/nanumsquareb"
                    android:paddingStart="20dp"
                    android:paddingEnd="20dp"
                    android:stateListAnimator="@null"
                    android:textAlignment="textStart"
                    android:textColor="#FFFFFF"
                    android:textSize="16dp"
                    android:visibility="invisible"
                    tools:ignore="SpUsage" />

                <Button
                    android:id="@+id/seeMore_Button_sub4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    android:background="@drawable/basic_button_gray"
                    android:fontFamily="@font/nanumsquareb"
                    android:paddingStart="20dp"
                    android:paddingEnd="20dp"
                    android:stateListAnimator="@null"
                    android:textAlignment="textStart"
                    android:textColor="#FFFFFF"
                    android:textSize="16dp"
                    android:visibility="invisible"
                    tools:ignore="SpUsage" />

                <Button
                    android:id="@+id/seeMore_Button_sub6"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    android:background="@drawable/basic_button_gray"
                    android:fontFamily="@font/nanumsquareb"
                    android:paddingStart="20dp"
                    android:paddingEnd="20dp"
                    android:stateListAnimator="@null"
                    android:textAlignment="textStart"
                    android:textColor="#FFFFFF"
                    android:textSize="16dp"
                    android:visibility="invisible"
                    tools:ignore="SpUsage" />

                <Button
                    android:id="@+id/seeMore_Button_sub8"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    android:background="@drawable/basic_button_gray"
                    android:fontFamily="@font/nanumsquareb"
                    android:paddingStart="20dp"
                    android:paddingEnd="20dp"
                    android:stateListAnimator="@null"
                    android:textAlignment="textStart"
                    android:textColor="#FFFFFF"
                    android:textSize="16dp"
                    android:visibility="invisible"
                    tools:ignore="SpUsage" />

            </LinearLayout>

            <Space
                android:layout_width="10dp"
                android:layout_height="match_parent" />

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <Button
                    android:id="@+id/seeMore_Button_sub1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    android:background="@drawable/basic_button_gray"
                    android:fontFamily="@font/nanumsquareb"
                    android:paddingStart="20dp"
                    android:paddingEnd="20dp"
                    android:stateListAnimator="@null"
                    android:textAlignment="textStart"
                    android:textColor="#FFFFFF"
                    android:textSize="16dp"
                    android:visibility="invisible"
                    tools:ignore="SpUsage" />

                <Button
                    android:id="@+id/seeMore_Button_sub3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    android:background="@drawable/basic_button_gray"
                    android:fontFamily="@font/nanumsquareb"
                    android:paddingStart="20dp"
                    android:paddingEnd="20dp"
                    android:stateListAnimator="@null"
                    android:textAlignment="textStart"
                    android:textColor="#FFFFFF"
                    android:textSize="16dp"
                    android:visibility="invisible"
                    tools:ignore="SpUsage" />

                <Button
                    android:id="@+id/seeMore_Button_sub5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    android:background="@drawable/basic_button_gray"
                    android:fontFamily="@font/nanumsquareb"
                    android:paddingStart="20dp"
                    android:paddingEnd="20dp"
                    android:stateListAnimator="@null"
                    android:textAlignment="textStart"
                    android:textColor="#FFFFFF"
                    android:textSize="16dp"
                    android:visibility="invisible"
                    tools:ignore="SpUsage" />

                <Button
                    android:id="@+id/seeMore_Button_sub7"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    android:background="@drawable/basic_button_gray"
                    android:fontFamily="@font/nanumsquareb"
                    android:paddingStart="20dp"
                    android:paddingEnd="20dp"
                    android:stateListAnimator="@null"
                    android:textAlignment="textStart"
                    android:textColor="#FFFFFF"
                    android:textSize="16dp"
                    android:visibility="invisible"
                    tools:ignore="SpUsage" />

                <Button
                    android:id="@+id/seeMore_Button_sub9"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    android:background="@drawable/basic_button_gray"
                    android:fontFamily="@font/nanumsquareb"
                    android:paddingStart="20dp"
                    android:paddingEnd="20dp"
                    android:stateListAnimator="@null"
                    android:textAlignment="textStart"
                    android:textColor="#FFFFFF"
                    android:textSize="16dp"
                    android:visibility="invisible"
                    tools:ignore="SpUsage" />

            </LinearLayout>

        </LinearLayout>

    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>