        android:id="@+id/EditToday_EditText_main"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:layout_weight="1"
        android:fontFamily="@font/arial"
        android:maxLength="12"
        android:maxLines="1"
        android:nextFocusDown="@id/EditToday_Button_add"
        android:singleLine="true"
        android:textColor="#000000"
        android:textSize="18dp"
        app:layout_constraintBottom_toTopOf="@id/EditToday_Button_add"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="Autofill,LabelFor,SpUsage,TextFields" />

    <Button
        android:id="@+id/EditToday_Button_add"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="20dp"
